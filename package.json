{
  "name": "mogara",
  "version": "1.3.2",
  "description": "Mogara! A lightweight custom markup language for web pages.",
  "type": "commonjs",
  "preferGlobal": true,
  "bin": {
    "mogara": "./bin/cli.js"
  },
  "files": [
    "assets/",
    "bin/",
    "lib/",
    "public/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node ./bin/cli.js",
    "build": "pkg .",
    "lint": "eslint src/index.js",
    "lint:fix": "eslint src/index.js --fix"
  },
  "author": "Syed Ismail <ismailbinmujeeb@gmail.com> (https://github.com/IsmailBinMujeeb)",
  "keywords": [
    "markup",
    "markdown",
    "text formatting",
    "lightweight markup",
    "custom markup",
    "documentation",
    "parser",
    "npm",
    "html",
    "css",
    "js"
  ],
  "license": "MIT",
  "devDependencies": {
    "eslint": "^9.19.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "opn": "^6.0.0",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ismailbinmujeeb/mogara.git"
  },
  "bugs": {
    "url": "https://github.com/ismailbinmujeeb/mogara/issues"
  },
  "homepage": "https://github.com/ismailbinmujeeb/mogara#readme",
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "contributors": [
    "Syed Ismail <ismailbinmujeeb@gmail.com> (https://github.com/IsmailBinMujeeb)"
  ]
}
